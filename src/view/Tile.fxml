<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" fx:id="tile" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
           minHeight="-Infinity" minWidth="-Infinity" onDragDetected="#onDragDetected" onDragDone="#onDragDone"
           prefHeight="50.0" prefWidth="50.0"
           style="-fx-background-color: lightyellow; -fx-border-color: #000000;" xmlns="http://javafx.com/javafx/16">
    <children>
        <Label fx:id="letter" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
               text="A">
            <font>
                <Font name="System Bold" size="30.0"/>
            </font>
        </Label>
        <Label fx:id="value" alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
               text="4">
            <padding>
                <Insets right="10.0" top="10.0"/>
            </padding>
        </Label>
    </children>
</StackPane>
